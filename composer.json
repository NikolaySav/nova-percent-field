{
    "name": "nsavinov/nova-percent-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "percent"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Nsavinov\\NovaPercentField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nsavinov\\NovaPercentField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "./vendor/bin/pint",
        "lint:check": "./vendor/bin/pint --test -v"
    }
}
